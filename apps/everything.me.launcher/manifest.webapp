{
  "name": "Webapps",
  "description": "Web apps launcher",
  "launch_path": "/index.html",
  "developer": {
    "name": "The Everything.me Team",
    "url": "http://everything.me/"
  },
  "permissions": [
    "browser",
    "mozbrowser",
    "systemXHR"
  ],
  "icons": {
    "60": "/icon.png"
  },
  "default_locale": "en-US",
  "activities": {
    "launch": {
      "filters": {
        "type": "url"
      }
    }
  }
}
